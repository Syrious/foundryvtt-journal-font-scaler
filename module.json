{
  "id": "journal-font-scaler",
  "title": "Journal Scaler",
  "description": "Use CTRL + mouse wheel to scale texts and images of journal entries",
  "version": "0.1.2",
  "compatibility": {
    "minimum": "10",
    "verified": "12"
  },
  "authors": [
    {
      "name": "Syrious",
      "discord": "syrious_"
    }
  ],
  "esmodules": [
    "/scripts/script.js",
    "/scripts/settings.js"
  ],
  "relationships": {
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module"
      }
    ]
  },
  "url": "https://github.com/Syrious/foundryvtt-journal-font-scaler",
  "manifest": "${module-endpoint}",
  "download": "${download-endpoint}"
}
